{
	"name": "Webcam extension test",
	"version": "1.0",
	"description": "Attempting to inject a frame with a webcam",
	"permissions": ["activeTab", "declarativeContent", "storage", "audioCapture"],
	"background": {
		"scripts": ["bundle.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"run_at": "document_start",
			"js": ["bundle.js"],
			"css": ["inject.css"]
		}
	],
	"page_action": {
		"default_popup": "popup.html"
	},
	"manifest_version": 2
}
